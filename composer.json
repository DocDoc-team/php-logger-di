{
    "name": "docdoc/psr-logger",
    "description": "Provide psr-logger for symfony DI container",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "log",
        "logger",
        "symfony-di"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.1",
        "docdoc/php-monolog-trace-processor": "^1.0",
        "symfony/expression-language": "^5.2"
    },
    "require-dev": {
        "ext-sockets": "*",
        "phpunit/phpunit": "^9.5",
        "docdoc/symfony-di-loader": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DocDoc\\Logger\\": "src"
        }
    }
}
